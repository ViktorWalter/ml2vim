s%@SIZEOF_LONG_INT@%$SIZEOF_LONG_INT%g
s%@SIZEOF_POINTER@%$SIZEOF_POINTER%g
s%@SIZEOF_LONG_LONG_INT@%$SIZEOF_LONG_LONG_INT%g
s%@HAVE_LONG_LONG_INT@%$HAVE_LONG_LONG_INT%g
